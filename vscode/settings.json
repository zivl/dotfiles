    "window.zoomLevel": 0,
    "gitlens.keymap": "none",
    "terminal.integrated.fontSize": 18,
    "material-icon-theme.showUpdateMessage": false,
    "gitlens.historyExplorer.enabled": true,
    "eslint.nodePath": "./node_modules/eslint-config-experts",
    "workbench.colorTheme": "Visual Studio Dark",
