{
    "workbench.iconTheme": "vscode-icons",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "files.autoSave": "onFocusChange",
    "editor.fontSize": 18,
    "editor.formatOnType": true,
    "prettier.printWidth": 120,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "workbench.startupEditor": "none",
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "cSpell.userWords": [
        "Backend",
        "CICD",
        "consts",
        "databinding",
        "dataset",
        "dataset's",
        "datetime",
        "dropdown",
        "elementory",
        "fedops",
        "grpc",
        "Guid",
        "Lightbox",
        "memberof",
        "Metasite",
        "middlewares",
        "Optimus",
        "Overdues",
        "Polyfill",
        "siteless",
        "testkit",
        "transpiled",
        "Truthy",
        "uncomment",
        "unconfigured",
        "Undeployment",
        "Unmount",
        "Unpublish",
        "upsert",
        "Validatable",
        "Webapp",
        "youtube",
        "zivl"
    ],
    "cSpell.language": "en",
    "search.exclude": {
      "**/dist": true,
      "**/coverage": true,
      "**/target": true,
      "**/node_modules": true,
      "**/bower_components": true,
      "**/*.rt.js": true,
      "**/.eslintcache": true
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "git-autoconfig.configList": [
        {
            "user.email": "levi.ziv@gmail.com",
            "user.name": "Ziv Levy"
        },
        {
            "user.email": "zivl@wix.com",
            "user.name": "Ziv Levy"
        }
    ],
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "editor.fontFamily": "'Ubuntu Mono derivative Powerline', Menlo, Monaco, 'Courier New', monospace",
    "gitlens.keymap": "none",
    "terminal.integrated.fontSize": 20,
    "material-icon-theme.showUpdateMessage": false,
    "eslint.nodePath": "<root>/.eslintrc.js",
    "wallaby.node": "/Users/zivl/.nvm/versions/node/v16.14.2/bin/node",
    "editor.minimap.enabled": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "search.followSymlinks": false,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "wallaby.startAutomatically": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.alwaysShowStatus": true,
    "workbench.colorTheme": "Brogrammer Plus",
    "diffEditor.ignoreTrimWhitespace": false,
    "keyboard.touchbar.enabled": false,
    "colorize.languages": [
        "javascript",
        "css",
        "sass",
        "scss",
        "less",
        "postcss",
        "sss",
        "stylus",
        "xml",
        "svg"
    ],
    "colorInfo.languages": [
        {
            "selector": "javascript",
            "colors": "css"
        },
        {
            "selector": "css",
            "colors": "css"
        },
        {
            "selector": "sass",
            "colors": "css"
        },
        {
            "selector": "scss",
            "colors": "css"
        },
        {
            "selector": "less",
            "colors": "css"
        }
    ],
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "telemetry.enableTelemetry": false,
    "autoimport.useSemiColon": false,
    "npm-intellisense.importLinebreak": "",
    "json.schemas": [
        {
            "name": "vscode-expo-manifest",
            "url": "file:///Users/zivl/Library/Application%20Support/Code/User/globalStorage/bycedric.vscode-expo/manifest-45.0.0.json",
            "fileMatch": [
                "app.json",
                "app.config.json"
            ]
        }
    ],
    "eslint.codeActionsOnSave.mode": "problems",
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    "vsicons.dontShowNewVersionMessage": true,
    "extensions.ignoreRecommendations": true,
    "git.confirmSync": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.explorerKind": "external",
    "npm.keybindingsChangedWarningShown": true,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "application.shellEnvironmentResolutionTimeout": 60,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false
    },
    "github.copilot.editor.enableAutoCompletions": true,
    "typescript.tsdk": "node_modules/typescript/lib"
}
